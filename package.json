{
  "name": "@metamask/providers",
  "version": "10.2.1",
  "description": "A JavaScript Ethereum provider that connects to the wallet over a stream.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "setup": "yarn install && yarn allow-scripts",
    "build": "tsc --project tsconfig.build.json",
    "test": "jest && jest-it-up",
    "test:watch": "jest --watch",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' '!.yarnrc.yml' --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "lint": "yarn lint:eslint && yarn lint:misc --check && yarn lint:dependencies --check && yarn lint:changelog",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write && yarn lint:dependencies && yarn lint:changelog",
    "prepublishOnly": "yarn build && yarn lint && yarn test",
    "build:clean": "rimraf dist && yarn build",
    "build:docs": "typedoc",
    "lint:changelog": "auto-changelog validate",
    "lint:dependencies": "depcheck && yarn dedupe",
    "prepack": "./scripts/prepack.sh"
  },
  "author": "MetaMask",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/providers.git"
  },
  "keywords": [
    "MetaMask",
    "Ethereum",
    "Provider"
  ],
  "bugs": {
    "url": "https://github.com/MetaMask/providers/issues"
  },
  "homepage": "https://github.com/MetaMask/providers#readme",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@metamask/object-multiplex": "^1.1.0",
    "@metamask/safe-event-emitter": "^3.0.0",
    "@types/chrome": "^0.0.136",
    "@types/webextension-polyfill": "^0.10.0",
    "detect-browser": "^5.2.0",
    "eth-rpc-errors": "^4.0.2",
    "extension-port-stream": "^2.0.1",
    "fast-deep-equal": "^2.0.1",
    "is-stream": "^2.0.0",
    "json-rpc-engine": "^6.1.0",
    "json-rpc-middleware-stream": "^4.2.1",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.1",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.23",
    "@types/pump": "^1.1.0",
    "@types/readable-stream": "^2.3.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-chrome": "^0.7.1",
    "jest-it-up": "^2.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.7.0",
    "typedoc": "^0.23.15",
    "typescript": "~4.8.4"
  },
  "lavamoat": {
    "allowScripts": {}
  },
  "packageManager": "yarn@3.2.1"
}
